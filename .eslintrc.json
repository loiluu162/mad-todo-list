{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "semi": "off",
        "comma-dangle": "off",
        "space-before-function-paren": "off",
        "lines-between-class-members" : ["error", "always"]
    }
}
